<?php

namespace KZ\KwizBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Position
 *
 * @ORM\Table(name="position")
 * @ORM\Entity(repositoryClass="KZ\KwizBundle\Repository\PositionRepository")
 */
class Position
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\ManyToOne(targetEntity="KZ\UserBundle\Entity\User")
     */
    private $user;

    /**
     * @ORM\ManyToOne(targetEntity="KZ\KwizBundle\Entity\Square")
     */
    private $square;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set user
     *
     * @param \KZ\UserBundle\Entity\User $user
     *
     * @return Position
     */
    public function setUser(\KZ\UserBundle\Entity\User $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \KZ\UserBundle\Entity\User
     */
    public function getUser()
    {
        return $this->user;
    }

    /**
     * Set square
     *
     * @param \KZ\UserBundle\Entity\User $square
     *
     * @return Position
     */
    public function setSquare(\KZ\KwizBundle\Entity\Square $square = null)
    {
        $this->square = $square;

        return $this;
    }

    /**
     * Get square
     *
     * @return \KZ\KwizBundle\Entity\Square
     */
    public function getSquare()
    {
        return $this->square;
    }
}
